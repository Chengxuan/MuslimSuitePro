		<aya index="1" text="سَبِّحِ اسْمَ رَبِّكَ الْأَعْلَى" bismillah="بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ" />
		<aya index="2" text="الَّذِي خَلَقَ فَسَوَّىٰ" />
		<aya index="3" text="وَالَّذِي قَدَّرَ فَهَدَىٰ" />
		<aya index="4" text="وَالَّذِي أَخْرَجَ الْمَرْعَىٰ" />
		<aya index="5" text="فَجَعَلَهُ غُثَاءً أَحْوَىٰ" />
		<aya index="6" text="سَنُقْرِئُكَ فَلَا تَنسَىٰ" />
		<aya index="7" text="إِلَّا مَا شَاءَ اللَّهُ ۚ إِنَّهُ يَعْلَمُ الْجَهْرَ وَمَا يَخْفَىٰ" />
		<aya index="8" text="وَنُيَسِّرُكَ لِلْيُسْرَىٰ" />
		<aya index="9" text="فَذَكِّرْ إِن نَّفَعَتِ الذِّكْرَىٰ" />
		<aya index="10" text="سَيَذَّكَّرُ مَن يَخْشَىٰ" />
		<aya index="11" text="وَيَتَجَنَّبُهَا الْأَشْقَى" />
		<aya index="12" text="الَّذِي يَصْلَى النَّارَ الْكُبْرَىٰ" />
		<aya index="13" text="ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ" />
		<aya index="14" text="قَدْ أَفْلَحَ مَن تَزَكَّىٰ" />
		<aya index="15" text="وَذَكَرَ اسْمَ رَبِّهِ فَصَلَّىٰ" />
		<aya index="16" text="بَلْ تُؤْثِرُونَ الْحَيَاةَ الدُّنْيَا" />
		<aya index="17" text="وَالْآخِرَةُ خَيْرٌ وَأَبْقَىٰ" />
		<aya index="18" text="إِنَّ هَٰذَا لَفِي الصُّحُفِ الْأُولَىٰ" />
		<aya index="19" text="صُحُفِ إِبْرَاهِيمَ وَمُوسَىٰ" />
